<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="qandaPU" transaction-type="JTA">
	 <jta-data-source>jdbc/qanda</jta-data-source>
	 <class>entity.Answer</class>
	 <class>entity.AnswerVoteUser</class>
	 <class>entity.AnswerVoteUserPK</class>
	 <class>entity.AnswerTargetComment</class>
	 <class>entity.Exercise</class>
	 <class>entity.ExerciseNum</class>
	 <class>entity.ExerciseNumPK</class>
	 <class>entity.ExerciseProblem</class>
	 <class>entity.ExerciseProblemPK</class>
	 <class>entity.ExerciseProblemStudent</class>
	 <class>entity.ExerciseProblemStudentPK</class>
	 <class>entity.ExerciseProblemStudentUsedTag</class>
	 <class>entity.ExerciseProblemStudentUsedTagPK</class>
	 <class>entity.ExerciseStudent</class>
	 <class>entity.ExerciseStudentPK</class>
	 <class>entity.FavHistory</class>
	 <class>entity.PostHistory</class>
	 <class>entity.Question</class>
	 <class>entity.QuestionFavUser</class>
	 <class>entity.QuestionFavUserPK</class>
	 <class>entity.QuestionTag</class>
	 <class>entity.QuestionTagPK</class>
	 <class>entity.QuestionTargetComment</class>
	 <class>entity.QuestionViewUser</class>
	 <class>entity.ReplyNotification</class>
	 <class>entity.Tag</class>
	 <class>entity.TagSearchUser</class>
	 <class>entity.TaglistComponent</class>
	 <class>entity.TaglistComponentPK</class>
	 <class>entity.User</class>
	 <exclude-unlisted-classes>false</exclude-unlisted-classes>
	 <properties>
            <property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect" />
            <property name="hibernate.show_sql" value="true" />
        </properties>
	</persistence-unit>
</persistence>
